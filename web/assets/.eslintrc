{
  "extends": [
    "eslint:recommended",
    "plugin:vue/strongly-recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "worker": false,
    "amd": false,
    "mocha": false,
    "jasmine": false,
    "phantomjs": false,
    "jquery": false,
    "prototypejs": false,
    "shelljs": false,
    "meteor": false,
    "mongo": false,
    "applescript": false,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-extra-semi": 1,
    "no-inner-declarations": 2,
    "curly": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-new-wrappers": 2,
    "no-with": 2,
    "no-undef": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "indent": [
      2,
      4
    ],
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "semi": [
      2,
      "never"
    ],
    "max-len": [
      1,
      80,
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": ["warn", {"allow": ["warn", "error"]}]
  }
}
